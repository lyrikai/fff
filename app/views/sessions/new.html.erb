

<section>

    <%= form_tag url_for(action: 'create'), method: "post" do %>

      <%= label_tag 'email' %>
      <%= text_field_tag 'email', @user %>
      <br /> <br />

      <%= label_tag 'pass' %>
      <%= password_field_tag 'pass', @user %>
      <br /> <br />

      <%= submit_tag "login" %>

    <% end %>

</section>


